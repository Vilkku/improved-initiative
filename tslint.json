{
    "rules": {
        "quotemark": [
            true,
            "double"
        ],
        "member-access": true,
        "no-inferrable-types": true,
        "no-internal-module": true,
        "no-var-keyword": true,
        "curly": [
            true,
            "ignore-same-line"
        ],
        "no-use-before-declare": true,
        "no-unused-variable": true,
        "semicolon": true,
        "ordered-imports": [
            true,
            {
                "import-sources-order": "lowercase-last",
                "named-imports-order": "lowercase-first"
            }
        ]
    }
}